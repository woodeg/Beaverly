{% extends 'base.html.twig' %}

{% block title %}Group{% endblock %}

{% block body %}
    <h1>Group</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ group.id }}</td>
            </tr>
            <tr>
                <th>GroupName</th>
                <td>{{ group.groupName }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ group.isActive ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_group_index') }}">back to list</a>

    <a href="{{ path('admin_group_edit', {'id': group.id}) }}">edit</a>

    {{ include('admin/group/_delete_form.html.twig') }}
{% endblock %}
