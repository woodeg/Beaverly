{% extends 'base.html.twig' %}

{% block title %}Type{% endblock %}

{% block body %}
    <h1>Type</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ type.id }}</td>
            </tr>
            <tr>
                <th>TypeName</th>
                <td>{{ type.typeName }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ type.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ type.createdAt ? type.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ type.updatedAt ? type.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_type_index') }}">back to list</a>

    <a href="{{ path('admin_type_edit', {'id': type.id}) }}">edit</a>

    {{ include('admin/type/_delete_form.html.twig') }}
{% endblock %}
